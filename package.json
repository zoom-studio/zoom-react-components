{
  "name": "@zoom-studio/zoom-react-components",
  "version": "0.0.0",
  "description": "The design language of Zoom",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "homepage": "http://zoom-studio.github.io/zoom-react-components",
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "private": false,
  "repository": {
    "url": "https://github.com/zoom-studio/zoom-react-components.git",
    "type": "git"
  },
  "author": {
    "name": "zoom-studio",
    "url": "http://zoom-studio.github.io/zoom-react-components"
  },
  "keywords": [
    "react",
    "react-component-lib",
    "storybook-react",
    "zoom-studio"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,scss}": [
      "yarn prettify:write"
    ],
    "*.{ts,tsx}": [
      "yarn lint:ts"
    ],
    "*.{css,scss}": [
      "yarn lint:scss"
    ]
  },
  "scripts": {
    "start": "yarn storybook dev -p 9998",
    "build": "yarn clean && rollup -c --bundleConfigAsCjs && yarn clean:removal-dists && yarn fix-sass-dist-file",
    "fix-sass-dist-file": "node ./source/scripts/rename-files-in-dist.js index.es.css index.css",
    "publish": "npm publish --access public",
    "deploy": "yarn run storybook build && gh-pages -d storybook-static",
    "clean": "rimraf dist tsconfig.tsbuildinfo storybook-static .rollup.cache",
    "clean:removal-dists": "rimraf dist/stories dist/fixtures dist/i18n",
    "lint:scss": "npx stylelint ./source/**/*.scss",
    "lint:ts": "eslint ./source --ext .ts,.tsx --fix --quiet",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "prepare": "husky install",
    "prettify:check": "npx prettier --check .",
    "prettify:write": "npx prettier --write .",
    "test": "jest --silent=true",
    "test:watch": "jest --watch --silent=false",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@floating-ui/react": "0.24.2",
    "@prezly/slate-lists": "0.84.0",
    "@tanstack/react-table": "8.9.1",
    "@tanstack/react-virtual": "^3.0.0-beta.54",
    "@viselect/react": "^3.2.7",
    "@zoom-studio/zoom-js-ts-utils": "file:../utils/dist",
    "lodash": "^4.17.21",
    "overlayscrollbars": "2.1.1",
    "overlayscrollbars-react": "^0.5.0",
    "react-advanced-cropper": "0.19.3",
    "react-avatar-editor": "^13.0.0",
    "react-hot-toast": "2.4.1",
    "react-menu-list": "https://github.com/zoom-studio/react-menu-list.git",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "6.11.2",
    "react-to-print": "2.14.12",
    "react-virtualized-auto-sizer": "1.0.18",
    "react-window": "1.8.9",
    "react-zoom-pan-pinch": "3.0.7",
    "slate": "0.94.1",
    "slate-history": "^0.93.0",
    "slate-react": "0.94.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "7.22.3",
    "@faker-js/faker": "8.0.2",
    "@rollup/plugin-commonjs": "25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "15.0.2",
    "@rollup/plugin-typescript": "11.1.1",
    "@rollup/pluginutils": "5.0.2",
    "@storybook/addon-a11y": "7.0.18",
    "@storybook/addon-actions": "7.0.18",
    "@storybook/addon-essentials": "7.0.18",
    "@storybook/addon-interactions": "7.0.18",
    "@storybook/addon-links": "7.0.18",
    "@storybook/addon-mdx-gfm": "7.0.18",
