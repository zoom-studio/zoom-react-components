{
  "name": "@zoom-studio/zoom-react-components",
  "version": "0.0.0",
  "description": "The design language of Zoom",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "homepage": "http://zoom-studio.github.io/zoom-react-components",
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "private": false,
  "repository": {
    "url": "https://github.com/zoom-studio/zoom-react-components.git",
    "type": "git"
  },
  "author": {
    "name": "zoom-studio",
    "url": "http://zoom-studio.github.io/zoom-react-components"
  },
  "keywords": [
    "react",
    "react-component-lib",
    "storybook-react",
    "zoom-studio"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,scss}": [
      "yarn prettify:write"
    ],
    "*.{ts,tsx}": [
      "yarn lint:ts"
    ],
    "*.{css,scss}": [
      "yarn lint:scss"
    ]
  },
  "scripts": {
    "start": "start-storybook --no-manager-cache -p 9998",
    "build": "yarn clean && rollup -c --bundleConfigAsCjs && yarn clean:removal-dists && yarn fix-sass-dist-file",
    "fix-sass-dist-file": "node ./source/scripts/rename-files-in-dist.js index.es.css index.css",
    "pub": "npm publish --access public",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "deploy": "yarn deploy-storybook",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist tsconfig.tsbuildinfo storybook-static .rollup.cache",
    "clean:removal-dists": "rimraf dist/stories dist/fixtures dist/i18n",
    "lint:scss": "npx stylelint ./source/**/*.scss",
    "lint:ts": "eslint ./source --ext .ts,.tsx --fix --quiet",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "prepare": "husky install",
    "prettify:check": "npx prettier --check .",
    "prettify:write": "npx prettier --write .",
    "test": "jest --silent=true",
    "test:watch": "jest --watch --silent=false",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@floating-ui/react": "^0.22.0",
    "@prezly/slate-lists": "^0.81.2",
    "@tanstack/react-table": "^8.8.4",
    "@tanstack/react-virtual": "^3.0.0-beta.54",
    "@zoom-studio/zoom-js-ts-utils": "file:../zoom-web-utils/dist",
    "lodash": "^4.17.21",
    "overlayscrollbars": "^2.0.3",
    "overlayscrollbars-react": "^0.5.0",
    "react-advanced-cropper": "^0.18.0",
    "react-avatar-editor": "^13.0.0",
    "react-hot-toast": "^2.4.0",
    "react-menu-list": "https://github.com/zoom-studio/react-menu-list.git",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "^6.4.2",
    "react-selectable": "^2.1.1",
    "react-to-print": "^2.14.11",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.8",
    "react-zoom-pan-pinch": "^2.2.0",
    "slate": "^0.94.0",
    "slate-history": "^0.93.0",
    "slate-react": "^0.94.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@faker-js/faker": "^7.6.0",
    "@rollup/plugin-commonjs": "^23.0.5",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@rollup/pluginutils": "^5.0.1",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-webpack4": "^6.5.12",
    "@storybook/manager-webpack4": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/react": "^18.0.21",
    "@types/react-avatar-editor": "^13.0.0",
    "@types/react-dom": "^18.0.6",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "babel-loader": "^8.2.5",
    "change-case": "^4.1.2",
    "css-loader": "^3.6.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-storybook": "^0.6.6",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "i18next": "^22.4.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.2.0",
    "lint-staged": "^13.0.3",
    "lorem-ipsum": "^2.0.8",
    "postcss": "^8.4.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "react-json-view": "^1.21.3",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^10.2.0",
    "storybook-addon-performance": "^0.16.1",
    "storybook-dark-mode": "^1.1.2",
    "style-loader": "^1.3.0",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
